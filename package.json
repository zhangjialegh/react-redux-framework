    "react-redux": "^5.0.7",
    "react-roll-container": "^1.0.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-sticky": "^6.0.3",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-immutable": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "2.3.0",
    "resolve": "1.6.0",
    "serve": "^6.5.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "0.6.2",
    "webpack": "4.17.1",
    "webpack-dev-server": "3.1.5",
    "webpack-manifest-plugin": "2.0.0",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "theme": {
    "brand-primary": "#D5A478",
    "brand-primary-tap": "#E4B992",
    "brand-success": "#29A520",
    "brand-success-tap": "#45C53B",
    "brand-error": "#FF5000",
    "brand-error-tap": "#FF6628",
    "color-text-base": "#333",
    "fill-disabled": "#BBBBBB"
  }
}
