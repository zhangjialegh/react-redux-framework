    "jest": "20.0.4",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mapbox-gl": "^0.50.0",
    "mini-css-extract-plugin": "^0.4.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "postcss-pxtorem": "^4.0.1",
    "precss": "^3.1.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "rc-form": "^2.2.4",
    "react": "16.4.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "16.4.2",
    "react-fastclick": "^3.0.2",
